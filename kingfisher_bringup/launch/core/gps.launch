<launch>
  <!-- GPS serial comms -->
  <group ns="gps">
    <node pkg="nmea_comms" type="nmea_serial_node" name="serial_node">
      <param name="port" value="/dev/ublox" />
      <param name="baud" value="115200" />
    </node>

    <!-- interpretation -->
    <node pkg="nmea_gps_driver" type="nmea_driver.py" name="nmea_topic_driver" />

    <!-- ENU translation -->
    <node pkg="enu" type="from_fix" name="enu_publisher" />

    <!-- Compute declination from GPS position -->
    <node pkg="declination" type="compute.py" name="compute_declination" cwd="node">
      <remap from="declination" to="/imu/declination" />
    </node>
  </group>
</launch>
